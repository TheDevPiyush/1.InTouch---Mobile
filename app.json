{
  "expo": {
    "name": "InTouch",
    "slug": "InTouch",
    "scheme": "InTouch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thedevpiyush.InTouch",
      "buildNumber": "1.0.0",
      "distribution": "internal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thedevpiyush.InTouch",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ca1320e9-02a4-4fc0-9ae6-0f92c92b0f0f"
      }
    }
  }
}